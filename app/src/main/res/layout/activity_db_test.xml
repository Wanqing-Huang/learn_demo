<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="12dp">

    <Button
        android:id="@+id/btn_insert_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="insert" />

    <Button
        android:id="@+id/btn_update_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="update" />

    <Button
        android:id="@+id/btn_query_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="query" />

    <Button
        android:id="@+id/btn_delete_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="delete" />

    <Button
        android:id="@+id/btn_one_to_one_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="一对一关系测试" />

    <Button
        android:id="@+id/btn_one_to_more_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="一对多关系测试" />

    <Button
        android:id="@+id/btn_more_to_more_test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="一对多关系测试" />


    <TextView
        android:id="@+id/txt_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>